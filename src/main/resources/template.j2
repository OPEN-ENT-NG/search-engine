{
  "name": "fr.openent~search-engine",
  "config": {
    "main" : "fr.openent.searchengine.SearchEngine",
    "port" : 8053,
    "csrf-token": {{ csrfToken | default('true') }},
    "app-name" : "Searchengine",
    "app-address" : "/searchengine",
    "app-icon" : "searchengine-large",
    "app-type" : "END_USER",
    "host": "https://{{ host }}",
    "ssl" : true,
    "auto-redeploy": false,
    {% if overrideTheme %} "override-theme": {{ override2d }}, {% endif %}
    "userbook-host": "https://{{ host }}",
    "integration-mode": "{{ integration_mode|default('HTTP') }}",
    "app-registry.port" : 8012,
    "mode" : "prod",
    "entcore.port" : 8009,
    "max-sec-time-allowed" : 4,
    "paging-size-per-collection" : 10,
    "search-word-min-size" : 4
  }
}